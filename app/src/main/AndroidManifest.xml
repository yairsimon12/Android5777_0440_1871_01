<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yair.travelagencies.EazyTour">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yair.travelagencies.EazyTour.controller.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yair.travelagencies.EazyTour.controller.RegistryActivity" />

        <provider
            android:name="com.yair.travelagencies.EazyTour.model.backend.TravelAgenciesContentProvider"
            android:authorities="com.yair.travelagencies"
            android:enabled="true"
            android:exported="true"></provider>
    </application>

</manifest>